{
  "root": "apps/front",
  "projectType": "application",
  "sourceRoot": "apps/front/src",
  "targets": {
    "build": {
      "executor": "@nx-plus/nuxt:browser",
      "options": {
        "buildDir": "dist/apps/front"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx-plus/nuxt:server",
      "options": {
        "browserTarget": "front:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "browserTarget": "front:build:production",
          "dev": false
        }
      }
    },
    "static": {
      "executor": "@nx-plus/nuxt:static",
      "options": {
        "browserTarget": "front:build:production"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/front/**/*.{ts,tsx,vue}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/front"],
      "options": {
        "jestConfig": "apps/front/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
